# Local development only
NODE_ENV=development
PORT=3000
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Auth0 (example placeholders)
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_AUDIENCE=https://api.example.com/
AUTH0_JWKS_URI=https://your-tenant.auth0.com/.well-known/jwks.json

# CockroachDB / Postgres-compatible URL
DATABASE_URL=postgresql://user:password@localhost:26257/app?sslmode=disable

# Redis
REDIS_URL=redis://localhost:6379

# Typesense
TYPESENSE_API_KEY=dev_key
TYPESENSE_HOST=localhost
TYPESENSE_PORT=8108
TYPESENSE_PROTOCOL=http

# Ably
ABLY_API_KEY=your.ably:api_key

# Stripe
STRIPE_WEBHOOK_SECRET=whsec_xxx
STRIPE_API_KEY=sk_test_xxx
